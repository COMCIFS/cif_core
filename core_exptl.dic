     group opertions of the space group
 
                     |  r11  r21  r31  |
              RT  =  |  r12  r22  r32  |
                     |  r13  r23  r33  |
;
    _description.common         'SymmetryRotMatrix'
    _name.category_id            equiv
    _name.object_id              RT
    _type.purpose                Quantity 
    _type.source                 Derived   
    _type.container              Matrix
    _type.contents               Real
    _type.dimension              [3,3]
     loop_
    _method.purpose   
    _method.expression
     Evaluation
;
        _symmetry_equiv.RT =  Transpose (_symmetry_equiv.R)
;
     save_
 
 
save_symmetry_equiv.Seitz_matrix
    _definition.id             '_symmetry_equiv.Seitz_matrix'
    _definition.update           2012-11-22
    _description.text
;
     A matrix containing the symmetry operations of a space group
     in 4x4 Seitz format.
 
                    |  r11  r12  r13  t1  |
       | R  T |     |  r21  r22  r23  t2  |
       | 0  1 |     |  r31  r32  r33  t3  |
                    |   0    0    0    1  |
;
    _description.common         'SymmetrySeitzMatrix'
    _name.category_id            equiv
    _name.object_id              Seitz_matrix
    _type.purpose                Quantity 
    _type.source                 Derived  
    _type.container              Matrix
    _type.contents               Real
    _type.dimension              [4,4]
     loop_
    _method.purpose   
    _method.expression
     Evaluation
;
   _symmetry_equiv.Seitz_matrix =  SeitzFromJones (_symmetry_equiv.pos_as_xyz)
;
     save_
 
 
