    With  r  as  refln

          _refln.index_m_list  =

                  [ r.index_m_1, r.index_m_2, r.index_m_3, r.index_m_4,
                    r.index_m_5, r.index_m_6, r.index_m_7, r.index_m_8 ]
; 

