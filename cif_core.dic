    _enumeration.range           0.:
    _units.code                  dalton
     loop_
    _method.purpose
    _method.expression
     Evaluation
;
    mass = 0.
 
    Loop t as atom_type  {
 
                   mass += t.number_in_cell * t.atomic_mass
    }
      _cell.atomic_mass = mass
;
     save_
 
 
