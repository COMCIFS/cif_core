;
    with c  as  cell

          _cell.metric_tensor = [[ c.vector_a*c.vector_a,
    c.vector_a*c.vector_b, c.vector_a*c.vector_c ],
     [ c.vector_b*c.vector_a, c.vector_b*c.vector_b, c.vector_b*c.vector_c ],
