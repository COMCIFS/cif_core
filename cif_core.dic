    _type.purpose                 Link
    _type.source                  Related
    _type.container               Single
    _type.contents                Word
    _method.purpose               Definition
    _method.expression
;
    with aca as audit_contact_author
    count = 0
    target = ''
    loop aa as audit_author {
        if (aca.name == aa.name) {
            count++
            target = aa.id
            }
        }
    if (count == 1) {
        _enumeration.default = target
        }
    else {
        _enumeration.default = missing
    }
;

