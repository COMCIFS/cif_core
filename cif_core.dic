_type.contents                          Real
_type.dimension                         '[3,3]'
loop_
  _method.purpose
  _method.expression
         Evaluation          
;
     with c  as  cell
 
           _cell.metric_tensor = [[ c.vector_a*c.vector_a, c.vector_a*c.vector_b, c.vector_a*c.vector_c ],
                                  [ c.vector_b*c.vector_a, c.vector_b*c.vector_b, c.vector_b*c.vector_c ],
                                  [ c.vector_c*c.vector_a, c.vector_c*c.vector_b, c.vector_c*c.vector_c ]]
