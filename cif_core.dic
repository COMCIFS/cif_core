;
     Function SymLat( s :[Single, Symop])  {  # symop string
 
            If (s[0] == ' ')  v = [ 5, 5, 5 ]
            Else              v = [ AtoI(s[2]), AtoI(s[3]), AtoI(s[4]) ]
 
                  SymLat  =  v - 5
     }
; 

