    _type.purpose                 Link
    _type.source                  Related
    _type.container               Single
    _type.contents                Word
    _method.purpose               Definition
    _method.expression
;
    with pca as publ_contact_author
    count = 0
    target = ''
    loop pa as publ_author {
        if (pca.name == pa.name) {
            count++
            target = pa.id
            }
        }
    if (count == 1) {
        _enumeration.default = target
        }
    else {
        _enumeration.default = missing
    }
;

