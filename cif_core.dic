    _name.object_id              R
    _type.purpose                Number
    _type.source                 Derived
    _type.container              Matrix
    _type.contents               Real
    _type.dimension              [3,3]
     loop_
    _method.purpose
    _method.expression
     Evaluation
;
                       sm =  _symmetry_equiv.Seitz_matrix
 
        _symmetry_equiv.R =  [[sm[0,0],sm[0,1],sm[0,2]],
                              [sm[1,0],sm[1,1],sm[1,2]],
                              [sm[2,0],sm[2,1],sm[2,2]]]
;
     save_
 
 
