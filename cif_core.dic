loop_
  _method.purpose
  _method.expression
         Evaluation          
;
     typelist  = List()
 
     Loop  a  as  atom_site  {
        type = AtomType ( a.label )
        If( type not in typelist )  typelist ++= type
      }
      For type in typelist {
                              atom_type(.symbol         = type,
                                        .number_in_cell = '?'   )
      }
; 
