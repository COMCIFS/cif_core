    _type.dimension              [4,4]
     loop_
    _method.purpose
    _method.expression
     Evaluation
;
   _symmetry_equiv.Seitz_matrix =  SeitzFromJones (_symmetry_equiv.pos_as_xyz)
;
     save_
 
 
